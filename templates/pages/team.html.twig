{% extends 'base.html.twig' %}

{% block title %}Teams{% endblock %}

{% block main %}
  <h2>Display Teams</h2>
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Team</th>
      </tr>
    </thead>
    <tbody>
      {% for team in teams %}
        <tr class="table-secondary">
          <td>{{team.teamName}}</td>
        </tr>
      {% endfor %}

    {% endblock %}
